<!--miniprogram/pages/basicComponent/main/main.wxml-->

  <text class="title">{{title}}</text>

<view class="containerStyle">
  <button class="btnStyle" size="mini" bind:tap="go2ComponetIcon" type="primary">{{compont_icon_btn_str}}</button>
  <button class="btnStyle" style="width:30%" bind:tap="go2ComponetProgress" type="primary">{{compont_progress_btn_str}}</button>
  <button class="btnStyle" style="width:30%" bind:tap="go2ComponetRichText" type="primary">{{compont_rich_text_btn_str}}</button>
</view>

<view class="containerStyle1">
  <button class="btnStyle1" size="mini"  bind:tap="go2ComponetView">{{compont_view_btn_str}}</button>
  <button class="btnStyle1" size="mini"  bind:tap="go2ComponetMoveableView">{{compont_moveable_view_str}}</button>
  <button class="btnStyle1" size="mini"  bind:tap="go2_Canvas">{{compont_canvas_str}}</button>
</view>




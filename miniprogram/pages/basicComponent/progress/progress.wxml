<!--miniprogram/pages/basicComponent/progress/progress.wxml-->
<text class="componettitle">{{title}}</text>
<view>原生progress</view>
<progress style=" margin-top: 25px; margin-left:20px;  margin-right:20px" bindtap="onProgressTap" percent="{{progressValue}}" show-info="true" stroke-width="10" activeColor="#10AEFF" 
backgroundColor="#f2f2f2" active-mode="forwards" active
bindactiveend="onProgressActiveEnd" border-radius="5"
>
</progress>

<button style="font-size:small" bindtap="onProgressReload1" size="small" type="primary">progress重新动起来</button>

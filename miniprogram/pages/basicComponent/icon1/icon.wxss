/* miniprogram/pages/basicComponent/icon.wxss */


.icon_list_Style {
  width:100%;
  white-space: normal;
  overflow-x: auto;
}

.view-item{
  width:20%;
  display:inline-block;
}


/*
* 精灵图
*/
.sprite_icon{
  height: 80px;
  width:80px;
  display:block;
  background: url("https://ydb-pic1.oss-cn-shanghai.aliyuncs.com/custom.png") -180px -310px;
}

/*
* 矢量图
*(svg 图转化为base64)
*/
.svg_icon{
  height: 16px;
  width:16px;
  display: block;
  background-repeat:no-repeat;
  background: url("data:image/svg+xml;base64,");

}